<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CountInPaymentISAs__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
ISPICKVAL(Status__c,"Payment") ||
ISPICKVAL(Status__c,"Internship"),
1,
0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This field is used in reports to count the In-Payment ISA's .</inlineHelpText>
    <label>Count In-Payment ISAs</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
